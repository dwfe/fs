{
  "name": "@do-while-for-each/fs",
  "description": "Working with the File System",
  "version": "1.2.11",
  "main": "dist/cjs/index.js",
  "types": "dist/types/index.d.ts",
  "homepage": "https://github.com/dwfe/fs",
  "bugs": "https://github.com/dwfe/fs/issues",
  "repository": {
    "type": "git",
    "url": "git@github.com:dwfe/fs.git"
  },
  "keywords": [
    "file system",
    "fs"
  ],
  "author": "Do|While|For|Each",
  "license": "MIT",
  "bin": {
    "dwfeFs": "dist/cjs/bin.js"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "clean": "dwfeFs cleanDir . --noConfirm --fileNames=\"dist;node_modules;package-lock.json\"",
    "build": "dwfeFs rmForce dist --noConfirm && tsc -b src/tsconfig.json",
    "types": "tsc -b config/tsconfig.types.json",
    "version": "npm run build && npm run types",
    "postversion": "git push",
    "deploy": "npm version patch && npm publish"
  },
  "dependencies": {
    "@do-while-for-each/log-node": "^1.0.9"
  },
  "devDependencies": {
    "@types/node": "^16.11.22"
  },
  "publishConfig": {
    "access": "public"
  }
}
